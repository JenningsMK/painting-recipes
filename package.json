  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "cypress": "^13.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^13.24.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "typescript": "^5.3.3"
  }
}
